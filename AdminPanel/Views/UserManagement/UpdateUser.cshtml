                                </label>
                                <div class="col-lg-9">
                                    <input type="tel" name="PhoneNo" id="PhoneNo" value="@(Model.UserEntityObj.PhoneNo)" class="form-control" placeholder="">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-form-label col-lg-3">
